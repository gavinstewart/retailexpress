<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <cataloginventory>
            <options>
                <eta_enabled>0</eta_enabled>
                <eta_button_title>Check ETA</eta_button_title>
                <eta_button_disclaimer>This date is when we expect to receive the product in our warehouse, then we will ship it to you.</eta_button_disclaimer>
            </options>
        </cataloginventory>
        <carriers>
            <skylink_pickup>
                <active>0</active>
                <model>RetailExpress\SkyLink\Model\Outlets\PickupCarrier</model>
                <title>Click and Collect</title>
                <specificerrmsg>Unfortunately, this is not available. For more, please your local store.</specificerrmsg>
                <sallowspecific>0</sallowspecific>
            </skylink_pickup>
        </carriers>
        <multishipping>
            <options>
                <checkout_multiple>0</checkout_multiple>
            </options>
        </multishipping>
        <skylink>
            <api>
                <version>2</version>
                <version_2_url>https://api-test.retailexpress.com.au/API/ecommerce.svc?singleWsdl</version_2_url>
            </api>
            <general>
                <sales_channel_id>1</sales_channel_id>
            </general>
            <customers>
                <customer_group_tax_class_id>3</customer_group_tax_class_id>
                <price_group_type>standard</price_group_type>
            </customers>
            <products>
                <name_attribute>description</name_attribute>
                <regular_price_attribute>default_price</regular_price_attribute>
                <special_price_attribute>discounted_price</special_price_attribute>
                <configurable_product_match_threshold>0.5</configurable_product_match_threshold>
                <composite_product_sync_rerun_threshold>300</composite_product_sync_rerun_threshold>
            </products>
            <debugging>
                <should_capture_logs>0</should_capture_logs>
                <uncaptured_logs_to_keep>1000</uncaptured_logs_to_keep>
                <captured_logs_to_keep>100000</captured_logs_to_keep>
                <purging_chance>0.25</purging_chance>
            </debugging>
        </skylink>
    </default>
</config>
