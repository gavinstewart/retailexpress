{
  "name": "re/skylink-magento2",
  "description": "Magento 2 extension to manage synchronisation with Retail Express.",
  "type": "magento2-module",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ben Corlett",
      "email": "ben@retailexpress.com.au"
    },
    {
      "name": "Tariq Hafeez",
      "email": "tariq@retailexpress.com.au"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repo.ecom.retailexpress.com.au/"
    },
    {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  ],
  "autoload": {
    "files": ["helpers.php", "registration.php"],
    "psr-4": {
      "RetailExpress\\SkyLink\\": ""
    }
  },
  "require": {
    "php": ">=5.5.0",
    "phpspec/phpspec": "^2.0|^3.0",
    "illuminate/support": ">=5.1 <5.3",
    "league/tactician": "^1.0",
    "league/tactician-bernard": "^0.5.0",
    "retail-express/command-bus-magento-2": "^1.9",
    "retail-express/skylink-eds": "^1.9",
    "retail-express/skylink-sdk": "^1.9"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.9.x-dev"
    }
  },
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
