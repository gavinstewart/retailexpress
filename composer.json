{
  "name": "retail-express/skylink-magento-2",
  "description": "Synchronisation extension for a Magento 2 based website to Retail Express.",
  "type": "magento2-module",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ben Corlett",
      "email": "ben@retailexpress.com.au"
    },
    {
      "name": "Tariq Hafeez",
      "email": "tariq@retailexpress.com.au"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.skystores.com.au/"
    },
    {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  ],
  "autoload": {
    "files": ["registration.php"],
    "psr-4": {
      "RetailExpress\\SkyLinkMagento2\\": ""
    }
  },
  "require": {
    "php": ">=5.5.0",
    "magento/framework": "^100.0.5",
    "retail-express/skylink": "^1.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
