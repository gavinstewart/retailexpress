{
  "name": "retail-express/skylink-magento-2",
  "description": "Magento 2 extension to manage synchronisation with Retail Express.",
  "type": "magento2-module",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ben Corlett",
      "email": "ben@retailexpress.com.au"
    },
    {
      "name": "Tariq Hafeez",
      "email": "tariq@retailexpress.com.au"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repo.ecom.retailexpress.com.au/"
    },
    {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  ],
  "autoload": {
    "files": ["helpers.php", "registration.php"],
    "psr-4": {
      "RetailExpress\\SkyLink\\": ""
    }
  },
  "require": {
    "php": ">=5.5.0",
    "henrikbjorn/phpspec-code-coverage": "^2.0",
    "illuminate/support": ">=5.1 <5.3",
    "league/tactician": "^1.0",
    "league/tactician-bernard": "^0.5.0",
    "magento/framework": "100.0.0|100.1.*|^101.0",
    "magento/module-backend": "100.0.*|100.1.*|^101.0",
    "magento/module-catalog": "100.0.*|101.0.*|^101.0",
    "magento/module-catalog-inventory": "100.0.*|100.1.*|^101.0",
    "magento/module-configurable-product": "100.0.*|100.1.*|^101.0",
    "magento/module-customer": "100.0.*|100.1.*|^101.0",
    "magento/module-eav": "100.0.*|100.1.*|^101.0",
    "magento/module-payment": "100.0.*|100.1.*|^101.0",
    "magento/module-sales": "100.0.*|100.1.*|^101.0",
    "retail-express/command-bus-magento-2": "^1.8",
    "retail-express/skylink-eds": "^1.8",
    "retail-express/skylink-sdk": "^1.8"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.8.x-dev"
    }
  },
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
